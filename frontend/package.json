{
  "name": "hobbit-tracker-frontend",
  "version": "0.2.1",
  "private": true,
  "type": "module",
  "scripts": {
    "watch": "vite build --watch --mode=develop",
    "build": "vue-tsc && vite build",
    "lint": "eslint src --ext .js --ext .vue --ext .ts --fix"
  },
  "dependencies": {
    "@fontsource/lato": "^4.2.2",
    "@heroicons/vue": "^2.0.10",
    "moment": "^2.29.4",
    "pinia": "^2.0.32",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue": "^3.4.15",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/language-server": "^1.8.27",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "register-service-worker": "^1.7.1",
    "vue-tsc": "^1.8.27",
    "workbox-build": "^6.5.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "@vue/typescript/recommended"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaVersion": 2020
    },
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "vue/no-reserved-component-names": [
        "off"
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
